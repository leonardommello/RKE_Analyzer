<?xml version="1.0"?>
<block>
  <name>ht6026</name>
  <key>ht6026_ht6026</key>
  <category>teste</category>
  <import>import ht6026</import>
  <make>ht6026.ht6026($input_cod)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>cod</name>
    <key>input_cod</key>
    <type>int</type>
  </param>


  <param>
    <name>Min Agreenments</name>
    <key>min_agreements</key>
    <type>int</type>
  </param>


    <param>
    <name>Debug</name>
    <key>debug</key>
    <type>bool</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
